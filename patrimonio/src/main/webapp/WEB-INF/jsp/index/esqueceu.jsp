<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>



<form action="" name="frm_esqueceu" id="frm_esqueceu"  method="post">
	<div class="panel panel-body login-form">
		<div class="text-center">
			<div class="icon-object border-warning text-warning"><i class="icon-spinner11"></i></div>
			<h5 class="content-group">Resetar Senha <small class="display-block">Nós lhe enviaremos instruções por email</small></h5>
		</div>

		<div class="form-group has-feedback">
			<input type="email" class="form-control" name="email" id="email" placeholder="Seu email">
			<div class="form-control-feedback">
				<i class="icon-mail5 text-muted"></i>
			</div>
		</div>

		<button type="submit" class="btn bg-success-400 btn-block">Resetar Senha <i class="icon-arrow-right14 position-right"></i></button>
	</div>
</form>
    